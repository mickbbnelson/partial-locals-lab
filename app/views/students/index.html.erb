<%= form_tag students_path, method: :get do %>
    <%= label_tag(:query, "Search Students:") %>
    <%= text_field_tag :query, params[:query] %>
    <br />
    <%= submit_tag "Submit" %>
<% end %>


<% @students.each do |s| %>
<%= render partial: "students/student", locals: {student: s} %>
<% end %>